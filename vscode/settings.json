{
    "git.confirmSync": false,
    "window.zoomLevel": 0,
    "breadcrumbs.enabled": true,
    "files.autoSave": "onFocusChange",
    "editor.cursorStyle": "block",
    "editor.fontWeight": "100",
    "explorer.openEditors.visible": 0,
    "explorer.confirmDelete": false,
    "editor.fontSize": 14,
    "python.venvFolders": [
        "tm/venv"
    ],
    "python.venvPath": "~/tm",
    "telemetry.enableTelemetry": false,
    "gitlens.blame.avatars": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.mode.statusBar.enabled": false,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "gitlens.advanced.useSymmetricDifferenceNotation": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.blame.compact": false,
    "gitlens.blame.heatmap.enabled": false,
    "gitlens.blame.highlight.enabled": false,
    "gitlens.blame.separateLines": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.hovers.changesDiff": "hunk",
    "gitlens.hovers.currentLine.details": false,
    "gitlens.hovers.currentLine.changes": false,
    "gitlens.hovers.avatars": false,
    "editor.autoClosingBrackets": "never",
    "git.detectSubmodulesLimit": 30,
    "git.autorefresh": false,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.enableNeovim": true,
    "go.autocompleteUnimportedPackages": true,
    "editor.rulers": [80,120],
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#383838"
    },
    "editor.scrollBeyondLastLine": false,
    "editor.minimap.enabled": false,
    "scm.diffDecorations": "gutter",
    "search.collapseResults": "alwaysExpand",
    "search.smartCase": true,
    "vim.useSystemClipboard": true,
    "editor.folding": false,
    "markdown.preview.markEditorSelection": false
}
